	$(document).ready(function () {
		$("#themeL").click(function (e) {
			e.preventDefault();
			$("#tmMode").addClass('light-theme');
			$("#tmMode").removeClass('dark-theme');
		})
		$("#themeD").click(function (e) {
			e.preventDefault();
			$("#tmMode").addClass('dark-theme');
			$("#tmMode").removeClass('light-theme');
		})
	})
</script>
